{
  "name": "videojs-resume",
  "version": "0.2.1",
  "description": "Resume playback in VideoJS",
  "scripts": {
    "build": "npm run sass; npm run build-dist",
    "sass": "mkdir -p dist/ && ./node_modules/.bin/node-sass --output-style compact ./src/videojs-resume.css dist/videojs-resume.min.css",
    "build-dist": "mkdir -p dist/ && ./node_modules/.bin/browserify ./src/videojs-resume.js  -o dist/videojs-resume.min.js"
  },
  "keywords": [
    "videojs",
    "plugin",
    "resume"
  ],
  "author": "Shawn Price",
  "license": "Apache-2.0",
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "browserify-shim": "^3.8.10",
    "jquery": "^2.1.4",
    "node-sass": "^3.4.2",
    "uglify-js": "^2.6.1",
    "video.js": "^5.3.0",
    "store": "^1.3.17"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:jQuery",
    "video.js": "global:videojs",
    "store": "global:store"
  }
}
