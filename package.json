{
  "name": "videojs-resume-gh-pages",
  "version": "0.1.0",
  "description": "Resume playback in VideoJS",
  "private": "true",
  "scripts": {
    "build": "npm run sass; npm run build-dist",
    "sass": "mkdir -p dist/ && ./node_modules/.bin/node-sass --output-style compact ./lib/videojs-resume.css dist/videojs-resume.min.css",
    "build-dist": "mkdir -p dist/ && ./node_modules/.bin/browserify ./lib/videojs-resume.js -s videojs-resume | ./node_modules/.bin/uglifyjs > dist/videojs-resume.min.js"
  },
  "keywords": [
    "videojs",
    "plugin",
    "resume"
  ],
  "author": "Shawn Price",
  "license": "Apache-2.0",
  "devDependencies": {
    "jquery": "^2.1.4",
    "store": "^1.3.17",
    "video.js": "^5.2.1",
    "videojs-resume": "^0.1.0"
  }
}
